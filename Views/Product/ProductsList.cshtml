@model ProductsListViewModel
 
@foreach(var p in Model.Products)
{
  @Html.Partial("ProductSummary", p);
}

<div page-model="@Model.PagingInfo" page-action="ProductsList" page-classes-enabled="true"
  page-class="waves-effect" page-class-normal="waves-teal btn-flat" page-class-selected="waves-light btn" page-url-category="@Model.CurrentCategory">
</div>
